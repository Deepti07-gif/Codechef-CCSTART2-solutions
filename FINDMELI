#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n,k,i;
	bool ispresent=false;
    cin>>n;
    cin>>k;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(i=0;i<n;i++){
        if(k==a[i]){
            ispresent=true;
            break;
        }
    }
    if(ispresent){
        printf("1");
    }
    else{
        printf("-1");
    }
	return 0;
}
